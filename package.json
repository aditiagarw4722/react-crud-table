{
  "name": "react-crud-table",
  "version": "0.0.16",
  "description": "A table that includes all the CRUD operations.",
  "main": "build/CRUDTable/index.js",
  "scripts": {
    "dev": "watch 'npm run build' src",
    "lint": "eslint src/**/*.js",
    "build": "babel src -d build",
    "test": "jest --env=jsdom --no-watchman --passWithNoTests",
    "types:check": "flow ./src",
    "test-watch": "npm test -- --watch",
    "prepublish": "npm run build",
    "release": "np"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xvicmanx/react-crud-table"
  },
  "keywords": [
    "CRUD",
    "react"
  ],
  "author": "Victor Trejo",
  "license": "ISC",
  "dependencies": {
    "bem-react-component-creator": "^1.2.0",
    "formik": "0.11.11",
    "moment": "^2.22.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-datetime": "^2.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-react-jsx": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "flow-bin": "^0.140.0",
    "jest": "^26.6.3",
    "np": "^2.18.2",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.2.0",
    "watch": "^1.0.2"
  }
}
